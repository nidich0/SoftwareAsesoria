          <GrpcServices>Both</GrpcServices>
        </Protobuf>
        <Protobuf Include="Calendarios\CalendariosApi.proto">
          <GrpcServices>Both</GrpcServices>
        </Protobuf>
        <Protobuf Include="Users\Models.proto" GrpcServices="Both" />
